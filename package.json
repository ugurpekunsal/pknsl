	"scripts": {
		"start": "react-scripts start",
		"prebuild": "mkdir -p build/css && cp -r public/css/* build/css/",
		"build": "react-scripts build && npm run optimize-css",
		"optimize-css": "css-purge -i build/css/default.css,build/css/layout.css,build/css/media-queries.css,build/css/magnific-popup.css -o build/css/styles.min.css",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy": "npm run build && firebase deploy"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
